{
  "name": "neolikotsi/laravel-smsportal",
  "description": "SMS Portal Notification Channel for laravel",
  "keywords": ["laravel", "notifications", "sms portal"],
  "license": "MIT",
  "authors": [{
    "name": "Neo Likotsi",
    "email": "nlikotsi5@gmail.com",
    "role": "Developer"
  }],
  "require": {
    "php" : ">=7.3",
    "illuminate/config": "~8.0",
    "illuminate/notifications": "~8.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "neolikotsi/php-smsportal": "^1.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Illuminate\\Notifications\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Illuminate\\Tests\\Notifications\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers" : [
        "Illuminate\\Notifications\\SMSPortalServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
